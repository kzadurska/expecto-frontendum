{
  "name": "expecto-frontendum",
  "private": true,
  "description": "A blog about all things frontend",
  "version": "0.1.0",
  "author": "Kasia Zadurska <kzadurska@gmail.com> (https://expectofrontendum.com)",
  "homepage": "https://expectofrontendum.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kzadurska/expecto-frontendum.git"
  },
  "bugs": {
    "url": "https://github.com/kzadurska/expecto-frontendum/issues"
  },
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "start:clean": "rm -rf public && rm -rf .cache && gatsby develop",
    "build": "rm -rf public && rm -rf .cache && gatsby build --prefix-paths",
    "serve": "gatsby serve --prefix-paths",
    "lint": "eslint --ext .js --ignore-pattern public . --fix",
    "lint:ci": "eslint --ext .js --ignore-pattern public .",
    "format": "prettier --write '{gatsby-*.js,{src,content}/**/*.{js}}'",
    "deploy": "npm run build && gh-pages -d public"
  },
  "dependencies": {
    "babel-plugin-styled-components": "1.13.2",
    "gatsby": "3.10.1",
    "gatsby-image": "3.10.0",
    "gatsby-plugin-feed": "3.10.0",
    "gatsby-plugin-google-analytics": "3.10.0",
    "gatsby-plugin-manifest": "3.10.0",
    "gatsby-plugin-offline": "4.10.0",
    "gatsby-plugin-react-helmet": "4.10.0",
    "gatsby-plugin-sharp": "3.10.2",
    "gatsby-plugin-styled-components": "4.10.0",
    "gatsby-remark-copy-linked-files": "4.7.0",
    "gatsby-remark-images": "5.7.0",
    "gatsby-remark-prismjs": "5.7.0",
    "gatsby-remark-responsive-iframe": "4.7.0",
    "gatsby-remark-smartypants": "4.7.0",
    "gatsby-source-filesystem": "3.10.0",
    "gatsby-transformer-json": "3.10.0",
    "gatsby-transformer-remark": "4.7.0",
    "gatsby-transformer-sharp": "3.10.0",
    "prismjs": "1.24.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "styled-components": "5.3.0",
    "styled-kit": "1.0.5",
    "typeface-cinzel": "1.1.13",
    "typeface-merriweather": "1.1.13"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "7.31.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-standard": "5.0.0",
    "gh-pages": "3.2.3",
    "prettier": "2.3.2"
  }
}
