{
  "name": "expecto-frontendum",
  "private": true,
  "description": "A blog about all things frontend",
  "version": "0.1.0",
  "contributors": [
    "Kasia Zadurska <kzadurska@gmail.com> (https://expectofrontendum.com)",
    "Robert Kirsz <robert.kirsz@gmail.com> (https://robertkirsz.com)"
  ],
  "homepage": "https://expectofrontendum.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kzadurska/expecto-frontendum.git"
  },
  "bugs": {
    "url": "https://github.com/kzadurska/expecto-frontendum/issues"
  },
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "start:clean": "rm -rf public && rm -rf .cache && gatsby develop",
    "build": "rm -rf public && rm -rf .cache && gatsby build",
    "serve": "gatsby serve",
    "lint": "eslint --ext .js --ignore-pattern public . --fix",
    "lint:ci": "eslint --ext .js --ignore-pattern public .",
    "format": "prettier --write '{gatsby-*.js,{src,content}/**/*.{js}}'"
  },
  "dependencies": {
    "babel-plugin-styled-components": "1.10.0",
    "gatsby": "2.0.111",
    "gatsby-image": "2.0.29",
    "gatsby-plugin-feed": "2.0.13",
    "gatsby-plugin-google-analytics": "2.0.13",
    "gatsby-plugin-manifest": "2.0.17",
    "gatsby-plugin-offline": "2.0.22",
    "gatsby-plugin-react-helmet": "3.0.6",
    "gatsby-plugin-sharp": "2.0.20",
    "gatsby-plugin-styled-components": "3.0.5",
    "gatsby-remark-copy-linked-files": "2.0.9",
    "gatsby-remark-images": "2.0.6",
    "gatsby-remark-prismjs": "3.2.4",
    "gatsby-remark-responsive-iframe": "2.0.9",
    "gatsby-remark-smartypants": "2.0.8",
    "gatsby-source-filesystem": "2.0.20",
    "gatsby-transformer-json": "2.1.8",
    "gatsby-transformer-remark": "2.2.3",
    "gatsby-transformer-sharp": "2.1.13",
    "prismjs": "1.15.0",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-helmet": "5.2.0",
    "styled-components": "4.1.3",
    "styled-kit": "0.3.4",
    "typeface-cinzel": "0.0.54",
    "typeface-montserrat": "0.0.54"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.13.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-standard": "4.0.0",
    "prettier": "1.16.4"
  }
}
